{
  "private": true,
  "name": "epherome",
  "version": "0.3.0",
  "description": "Powerful Minecraft Launcher",
  "main": "dist/main.js",
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.ts",
    "build-native": "cargo-cp-artifact -nc dist/native.node -- cargo build --message-format=json-render-diagnostics",
    "electron-rebuild": "electron-rebuild dist/native.node",
    "serve": "node --unhandled-rejections=strict scripts/build.js dev renderer watch",
    "electron": "node --unhandled-rejections=strict scripts/build.js dev main && electron . --remote-debugging-port=9222 --inspect=9223",
    "dev": "concurrently --kill-others -n esbuild,electron 'npm:serve' 'npm:electron'",
    "prod": "node --unhandled-rejections=strict scripts/build.js prod all",
    "eslint": "eslint ./src ./test --ext .js,.ts,.tsx --no-error-on-unmatched-pattern",
    "prettier": "prettier ./src ./test --write --no-error-on-unmatched-pattern",
    "check": "npm run eslint && npm run prettier && tsc --noEmit && cargo check",
    "package": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ResetPower/epherome.git"
  },
  "author": "ResetPower <resetpower20@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ResetPower/epherome/issues"
  },
  "build": {
    "productName": "Epherome",
    "copyright": "Copyright (C) 2021 ResetPower.",
    "artifactName": "Epherome-v${version}.${ext}",
    "directories": {
      "output": "package"
    },
    "asar": true,
    "compression": "maximum",
    "icon": "src/assets/Epherome.png",
    "win": {
      "target": "msi"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "tar.gz",
        "snap"
      ]
    },
    "files": [
      "dist/",
      "!node_modules"
    ]
  },
  "homepage": "https://github.com/ResetPower/epherome#readme",
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/react-transition-group": "^4.4.3",
    "@types/which": "^2.0.1",
    "@types/winreg": "^1.2.31",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "cargo-cp-artifact": "^0.1.5",
    "concurrently": "^6.2.2",
    "electron": "^15.0.0",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.3",
    "esbuild": "^0.13.3",
    "esbuild-postcss": "^0.0.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mocha": "^9.1.2",
    "postcss": "^8.3.8",
    "postcss-import": "^14.0.2",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.16",
    "ts-node": "^10.1.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.1",
    "log4js": "^6.3.0",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "nanoid": "^3.1.28",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-transition-group": "^4.4.2"
  }
}
