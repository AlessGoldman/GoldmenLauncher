{
  "private": true,
  "name": "epherome",
  "version": "0.1.0",
  "description": "Powerful Minecraft Launcher",
  "main": "dist/main.js",
  "scripts": {
    "serve": "webpack serve --env dev --env process='renderer'",
    "electron": "webpack build --env dev --env process='main' && electron . --remote-debugging-port=9222",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "eslint": "eslint src/**/*.{ts,tsx} test/**/*.{ts,tsx} --no-error-on-unmatched-pattern",
    "prettier": "prettier src/**/*.{ts,tsx} test/**/*.{ts,tsx} --write --no-error-on-unmatched-pattern",
    "dev": "concurrently -n wdm,ele 'npm:serve' 'npm:electron'",
    "prod": "webpack build --env prod --env process='all'",
    "package": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ResetPower/epherome.git"
  },
  "author": "ResetPower <resetpower20@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ResetPower/epherome/issues"
  },
  "build": {
    "extraMetadata": {
      "name": "Epherome"
    },
    "productName": "Epherome",
    "copyright": "Copyright (C) 2021 ResetPower.",
    "artifactName": "Epherome-${version}.${ext}",
    "directories": {
      "output": "package"
    },
    "asar": true,
    "compression": "normal",
    "icon": "assets/Epherome.png",
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "dmg",
      "category": ""
    },
    "linux": {
      "target": "tar.gz"
    },
    "files": [
      "dist/",
      "!node_modules"
    ]
  },
  "homepage": "https://github.com/ResetPower/epherome#readme",
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-rc.2",
    "@types/css-minimizer-webpack-plugin": "^3.0.2",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mini-css-extract-plugin": "^2.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.10",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-transition-group": "^4.4.2",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^3.11.5",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "concurrently": "^6.2.0",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "electron": "^13.1.8",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.1.0",
    "mocha": "^9.0.3",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "react-refresh": "^0.10.0",
    "tailwindcss": "^2.2.7",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "find-java-home": "^1.2.1",
    "got": "^11.8.2",
    "log4js": "^6.3.0",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "node-stream-zip": "^1.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-transition-group": "^4.4.2"
  }
}
