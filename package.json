{
  "name": "epherome",
  "version": "0.0.2",
  "description": "Powerful Minecraft Launcher",
  "main": "dist/main.js",
  "scripts": {
    "start": "webpack --config webpack/dev-main.js && electron . --remote-debugging-port=9222",
    "dev": "webpack serve --config webpack/dev-renderer.js",
    "test": "jest",
    "pack": "webpack --config webpack/prod.js && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ResetPower/epherome.git"
  },
  "author": "ResetPower <resetpower20@gmail.com>",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ResetPower/epherome/issues"
  },
  "homepage": "https://github.com/ResetPower/epherome#readme",
  "build": {
    "productName": "Epherome",
    "appId": "com.epherome",
    "copyright": "Copyright (c) 2021 ResetPower. All rights reserved.",
    "compression": "maximum",
    "directories": {
      "output": "package"
    },
    "asar": true,
    "win": {
      "target": "portable"
    },
    "files": [
      "dist/",
      "!node_modules/"
    ]
  },
  "jest": {
    "runner": "jest-electron/runner",
    "testEnvironment": "jest-electron/environment",
    "preset": "ts-jest",
    "testRegex": "(/test/.*\\.(test|spec))\\.ts?$"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^24.9.0",
    "jest-electron": "^0.1.11",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^9.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "*",
    "electron-log": "^4.3.4",
    "history": "^4.10.1",
    "json5": "^2.2.0",
    "material-design-icons": "^3.0.1",
    "node-stream-zip": "^1.13.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1"
  }
}
