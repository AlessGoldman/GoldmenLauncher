{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Electron Main",
      "port": 9223,
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Electron Render",
      "port": 9222,
      "webRoot": "${workspaceFolder}"
